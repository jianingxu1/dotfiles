# Lots of junk goes in here.
/caches/
/backups/

# Top secret stuff!
/link/.ssh/id_*
/link/.ssh/*.pem
/link/.ssh/known_hosts*

# Don't commit Vim plugins, vim-plug will install them.
/link/.vim/plugged
# Don't commit plug.vim; it's installed by .vimrc or init script and updated by PlugUpgrade.
/link/.vim/autoload/plug.vim

# Don't commit rbenv temp files.
/link/.rbenv/shims/
/link/.rbenv/versions/
/link/.rbenv/version

# Don't commit tmux plugins, tpm will install them.
/link/.tmux/plugins

# Don't commit tmux resurrect state.
/link/.tmux/resurrect

# Ignore any file with LOCAL in the name (good for per-machine vars/functions)
*LOCAL*

